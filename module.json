{
    "name": "mana-circles",
    "title": "Patria Mori Mana Circles add-on module",
    "description": "This module extends the mana-base module by adding support for circles and affinities.",
    "authors": [
        {
            "name": "Jonas Drageset Lindaas"
        }
    ],
    "version": "0.1.1",
    "compatibility": {
        "minimum": 10,
        "verified": 10,
        "maximum": 11
    },
    "scripts": [
        "scripts/moduleConfig.js",
        "scripts/moduleHooks.js",
        "scripts/moduleSettings.js",
        "scripts/api/spellApi.js",
        "scripts/model/circle.js",
        "scripts/model/affinity.js",
        "scripts/model/affinitySet.js",
        "scripts/model/spell.js",
        "scripts/definitions/circleDef.js",
        "scripts/definitions/spellDef.js",
        "scripts/utils/affinityUtils.js",
        "scripts/utils/moduleUtils.js",
        "scripts/utils/spellEngine.js",
        "scripts/utils/updateUtils.js",
        "scripts/view/customTabUI.js"
    ],
    "languages": [
        {
            "lang": "en",
            "name": "English",
            "path": "languages/en.json"
        }
    ],
    "styles": [
        "styles/mana-circles.css"
    ],
    "url": "https://github.com/Patria-Mori/mana-circles",
    "manifest": "https://raw.githubusercontent.com/Patria-Mori/mana-circles/main/module.json",
    "download": "https://github.com/Patria-Mori/mana-circles/releases/download/v0.1.1/module.zip",
    "relationships": {
        "systems": [
            {
                "id": "dnd5e",
                "type": "system",
                "manifest": "https://raw.githubusercontent.com/foundryvtt/dnd5e/master/system.json",
                "compatibility": {
                    "verified": "2.1.5"
                }
            }
        ],

        "requires": [
            {
                "id": "mana-base",
                "type": "module",
                "manifest": "https://raw.githubusercontent.com/Patria-Mori/mana-base/master/module.json",
                "compatibility": {
                    "verified": "0.2.1"
                }
            }
        ]
    }
}